{
  "name": "signal",
  "version": "1.0.0",
  "description": "Reactive tool",
  "scripts": {
    "start": "node index",
    "cov": "npm run coverage",
    "coverage": "nyc mocha -b",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run coverage && npm run report"
  },
  "author": "spirin.vladimir@gmail.com",
  "devDependencies": {
    "coveralls": "^3.0.3",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "statements": 9,
    "branches": 8,
    "functions": 8,
    "lines": 9,
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  }
}
